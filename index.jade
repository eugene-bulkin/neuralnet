html
  head
    script(src="http://code.jquery.com/jquery-1.10.1.min.js")
    script(src="neuralnet.js", type="text/javascript")
    link(href="style.css", rel="stylesheet")
    title Neural Network
  body
    #wrap
      svg#canvas
        defs
          marker#arrowEnd(viewBox="0 0 10 10", refX=8, refY=5, markerWidth=6, markerHeight=3, orient="auto", markerUnits="strokeWidth")
            path(d="M 0 0 L 10 5 L 0 10 z")
          marker#arrowEndRed(viewBox="0 0 10 10", refX=8, refY=5, markerWidth=6, markerHeight=3, orient="auto", markerUnits="strokeWidth", fill="#f00")
            path(d="M 0 0 L 10 5 L 0 10 z")
        g
    #form
    button#reinit(type='submit') Reinitialize
    div#forward
      ul
      button(type='button') Forward Run